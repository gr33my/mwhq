{
  "name": "mechwarriorhq",
  "version": "1.0.0",
  "description": "A web project using React.js, Tailwind CSS, and MariaDB. Trying to rekindle my love for development by taking it back to the OG.",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "client": "npm start --prefix client",
    "build": "npm run build --prefix client",
    "test": "npm test --prefix client",
    "dev": "concurrently \"npm run start\" \"npm run client\""
  },
  "dependencies": {
    "concurrently": "^6.2.1",
    "express": "^4.17.1",
    "mariadb": "^2.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.5",
    "tailwindcss": "^2.2.19"
  },
  "keywords": [
    "react",
    "tailwindcss",
    "mariadb",
    "web development"
  ],
  "author": "Graeme Reed",
